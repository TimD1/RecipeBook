    "recipeIngredient": [
        "3 eggs",
        "1 cup brown sugar",
        "3/4 tsp vanilla",
        "1 cup granulated sugar",
        "2 tsp baking soda",
        "1/2 cup butter",
        "2 cups peanut butter",
        "4 1/2 cups oatmeal",
        "1/2 cup chocolate chips (chocolate chunks)",
        "1/2 cup M and Ms",
        "1/8 cup chopped nuts"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Combine all ingredients. Use an ice cream scoop to form balls of dough, then gently flatten dough on cookie sheet. Bake at 350 for 10-12 minutes."
