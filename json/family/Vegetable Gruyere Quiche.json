{
    "@context": "https://schema.org",
    "@type": "Recipe",
    "name": "Vegetable Gruyere Quiche",
    "author": {
        "@type": "Person",
        "name": "Sue Dunn"
    },
    "description": "also known as `stinky cheese quiche'",
    "recipeCategory": "quiches",
    "recipeYield": "yield",
    "recipeIngredient": [
        "2 cups leek",
        "4 cups zucchini",
        "4 cups yellow squash",
        "2 cups red bell pepper",
		"2 frozen deep-dish pie crusts",
        "4 eggs",
        "2 egg white",
        "4 Tbsp milk",
		"2 tsp Italian seasoning",
		"1 tsp salt",
		"1/2 tsp black pepper",
		"1/2 cup shredded Gruyere cheese"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a large pan, bring 2 inches of water to a boil. Add leeks; cover and simmer for 5 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Add zucchini, squash, and bell pepper. Simmer another 5 minutes. Drain well and pat vegetables dry."
        },
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 375F. Spoon vegetables into pie shells."
        },
        {
            "@type": "HowToStep",
            "text": "Combine eggs, egg whites, milk, and spices; stir well until mixed."
        },
        {
            "@type": "HowToStep",
            "text": "Pour into pie shell. Bake at 375F for 30 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Sprinkle with cheese. Bake for an additional 5 minutes or until cheese is melted."
        }
    ],
    "image": [
