{
    "@context": "https://schema.org",
    "@type": "Recipe",
    "name": "Pumpkin Dip",
    "author": {
        "@type": "Person",
        "name": "Donna Knights"
    },
    "description": "Half of this recipe is usually enough",
    "recipeCategory": "appetizers",
    "recipeYield": "yield",
    "recipeIngredient": [
        "2 cups confectioners sugar",
        "8 oz cream cheese, softened",
        "15 oz canned pumpkin",
        "1 tsp cinnamon",
        "1/2 tsp ginger",
        "1 dash nutmeg"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Beat all ingredients until smooth, and refrigerate."
        },
        {
            "@type": "HowToStep",
            "text": "Serve with ginger snaps/apples, etc."
        }
    ],
