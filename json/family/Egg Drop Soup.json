{
    "@context": "https://schema.org",
    "@type": "Recipe",
    "name": "Egg Drop Soup",
    "author": {
        "@type": "Person",
        "name": "Carolyn Benjamin"
    },
    "description": "description of recipe",
    "recipeCategory": "soups",
    "recipeYield": "yield",
    "recipeIngredient": [
        "3 cups chicken stock",
        "1 egg, beaten",
        "1 scallion, chopped",
        "1 Tbsp cornstarch",
        "1 Tbsp water"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Put stock on high heat. While boiling, stir in cornstarch until stock thickens and becomes clear."
        },
        {
            "@type": "HowToStep",
            "text": "Slowly pour in egg and stir once gently. Turn off heat. Pour in bowls and top with chopped scallions."
        }
    ],
