{
    "@context": "https://schema.org",
    "@type": "Recipe",
    "name": "Spinach Quiche",
    "author": {
        "@type": "Person",
        "name": "Sue Dunn"
    },
    "description": "description of recipe",
    "recipeCategory": "quiches",
    "recipeYield": "2 quiches",
    "recipeIngredient": [
        "2 frozen deep dish pie crusts",
        "2 (12 oz) boxes frozen chopped spinach",
        "1 ham steak, diced",
        "1 chopped vidalia (white) onion",
        "2 cups cheddar cheese, shredded",
        "9 eggs",
        "1 cup milk",
        "1 tsp salt",
        "ground pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat oven to 375F."
        },
        {
            "@type": "HowToStep",
            "text": "Thaw spinach in microwave. When thawed, totally squeeze out all moisture."
        },
        {
            "@type": "HowToStep",
            "text": "In a medium bowl, whisk together the eggs, milk, salt, and pepper. Thoroughly stir in the spinach, cheese, onion, and ham. Pour half into each pie crust, and bake for 30-40 minutes."
        }
    ],
    "image": [
